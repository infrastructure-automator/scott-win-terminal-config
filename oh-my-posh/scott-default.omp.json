{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "os",
                    "background": "#ffa500",
                    "foreground": "#193549",
                    "leading_diamond": "\ue0b6",
                    "style": "diamond",
                    "template": "{{ .Icon }} "
                }, 
                {
                    "type": "shell",
                    "background": "#ffa500",
                    "foreground": "#193549",
                    "powerline_symbol": "",
                    "style": "powerline",
                    "template": "{{ .Name }} "
                },
                {
                    "type": "path",
                    "background": "#8800dd",
                    "foreground": "#eeeeee",
                    "powerline_symbol": "\ue0b0",
                    "properties": {
                        "style": "folder"
                    },
                    "style": "powerline",
                    "template": " \ue5ff {{ .Path }} "
                },
                {
                    "type": "time",
                    "background": "#ff479c",
                    "foreground": "#ffffff",
                    "powerline_symbol": "\ue0b0",
                    "properties": {
                        "time_format": "15:04:05"
                    },
                    "style": "powerline"
                },
                {
                    "foreground": "#193549",
                    "powerline_symbol": "\ue0b0",
                    "background": "#fffb38",
                    "type": "git",
                    "style": "powerline",
                    "background_templates": [
                        "{{ if .Rebase }}#F59E0B{{ end }}",
                        "{{ if .Merge }}#FCA5A5{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#F472B6{{ end }}",
                        "{{ if gt .Ahead 0 }}#C4B5FD{{ end }}",
                        "{{ if gt .Behind 0 }}#93C5FD{{ end }}",
                        "{{ if or .Working.Changed .Staging.Changed }}#FCD34D{{ end }}"
                    ],
                    "template": " {{ if hasPrefix \"feat/\" .Ref }} {{ end }}{{ if hasPrefix \"fix/\" .Ref }} {{ end }}{{ url .UpstreamIcon .UpstreamURL }} {{ trunc 32 .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Rebase }} 󰙯 {{ .Rebase.Current }}/{{ .Rebase.Total }}{{ end }}{{ if .Merge }} 󰘬 {{ end }}{{ if .Detached }} {{ trunc 7 .Commit.Sha }}{{ end }}{{ if or .Working.Changed .Staging.Changed }}  {{ .Working.String }}{{ if and .Working.Changed .Staging.Changed }}|{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} ",
                    "properties": {
                        "fetch_status": true,
                        "fetch_upstream_icon": true,
                        "branch_template": "{{ trunc 35 .Branch }}"
                    }
                },
                {
                    "type": "executiontime",
                    "background": "#555555",
                    "foreground": "#ffffff",
                    "powerline_symbol": "\ue0b0",
                    "properties": {
                        "threshold": 0,
                        "style": "round"
                    },
                    "style": "powerline",
                    "template": " {{ .FormattedMs }} "
                },
                {
                    "type": "root",
                    "background": "#ffffff",
                    "foreground": "#193549",
                    "powerline_symbol": "\ue0b0",
                    "style": "powerline",
                    "template": " \uf0e7 "
                },
                {
                    "type": "status",
                    "background": "#00a000",
                    "background_templates": [
                        "{{ if gt .Code 0 }}#c00000{{ end }}"
                    ],
                    "foreground": "#eeeeee",
                    "properties": {
                        "always_enabled": true
                    },
                    "style": "diamond",
                    "template": " {{ if gt .Code 0 }}\uf421{{ else }}\u2665{{ end }} ",
                    "trailing_diamond": "\ue0b4"
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "type": "text",
                    "foreground": "#f1184c",
                    "style": "plain",
                    "template": "\u276f"
                }
            ]
        }
    ],
    "final_space": true,
    "version": 2
}